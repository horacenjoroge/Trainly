{
  "name": "Trainly",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2"
  },
  "dependencies": {
    "@expo/config-plugins": "~54.0.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/drawer": "6.6.6",
    "@react-navigation/native": "6.1.8",
    "@react-navigation/stack": "6.3.17",
    "@reduxjs/toolkit": "1.9.2",
    "axios": "^1.8.4",
    "date-fns": "^4.1.0",
    "expo": "^54.0.25",
    "expo-background-fetch": "~14.0.8",
    "expo-constants": "~18.0.10",
    "expo-file-system": "~19.0.19",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-location": "~19.0.7",
    "expo-mail-composer": "~15.0.7",
    "expo-media-library": "~18.2.0",
    "expo-notifications": "~0.32.13",
    "expo-secure-store": "~15.0.7",
    "expo-sensors": "~15.0.7",
    "expo-sharing": "~14.0.7",
    "expo-sms": "~14.0.7",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-task-manager": "~14.0.8",
    "expo-updates": "~29.0.13",
    "lodash": "^4.17.21",
    "metro": "^0.83.1",
    "metro-config": "^0.82.0",
    "metro-resolver": "^0.82.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-animatable": "^1.3.3",
    "react-native-chart-kit": "^6.12.0",
    "react-native-elements": "^3.4.3",
    "react-native-event-listeners": "^1.0.7",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-share": "^12.0.3",
    "react-native-svg": "15.12.1",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-worklets": "^0.6.1",
    "react-redux": "8.0.5",
    "redux": "4.2.1",
    "redux-persist": "^6.0.0",
    "undici": "^7.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@testing-library/react-native": "^12.4.3",
    "@testing-library/jest-native": "^5.4.3",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.0",
    "react-test-renderer": "19.1.0"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|react-native-worklets)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "<rootDir>/__tests__/setup.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx|js)",
      "**/*.(test|spec).(ts|tsx|js)"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/android/**",
      "!**/ios/**",
      "!**/dist/**",
      "!**/coverage/**",
      "!**/*.config.js",
      "!**/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    }
  },
  "private": true
}
